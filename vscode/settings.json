{
	"files.exclude": {
		"**/.git": false
	},
	"search.exclude": {
		"**/node_modules": true
	},
	"debug.allowBreakpointsEverywhere": true,
	"editor.wordWrap": "on",
	"editor.smoothScrolling": true,
	"editor.suggestSelection": "first",
	"editor.fontFamily": "JetBrains Mono, Fira Code,  Menlo, Monaco, 'Courier New', monospace",
	"editor.fontLigatures": "tab",
	"editor.minimap.enabled": false,
	"editor.formatOnSave": true,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"workbench.iconTheme": "material-icon-theme",
	"workbench.startupEditor": "none",
	"workbench.colorCustomizations": {
		"gitlens.trailingLineForegroundColor": "#999999",
		"gitlens.lineHighlightOverviewRulerColor": "#ff0000"
	},
	"workbench.editorAssociations": [
		{
			"viewType": "jupyter.notebook.ipynb",
			"filenamePattern": "*.ipynb"
		}
	],
	"task.problemMatchers.neverPrompt": {
		"shell": true
	},
	"terminal.integrated.shell.osx": "/bin/zsh",
	"terminal.integrated.inheritEnv": false,
	"git.autofetch": true,
	"git.confirmSync": false,
	"prettier.configPath": "/Users/bilal/.prettierrc",
	"gitlens.currentLine.scrollable": false,
	"sync.gist": "4ae35fb5439214fac281eda4f924e619",
	"lldb.launch.expressions": "simple",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"python.languageServer": "Pylance",
	"python.showStartPage": false,
	"python.pythonPath": "/Users/bilal/miniconda3/envs/py38/bin/python",
	"jupyter.textOutputLimit": 0,
	"jupyter.askForKernelRestart": false,
	"jupyter.alwaysTrustNotebooks": true,
	"rust-analyzer.debug.openDebugPane": true,
	"rust-analyzer.checkOnSave.command": "clippy",
	"C_Cpp.default.intelliSenseMode": "clang-x64",
	"[python]": {
		"editor.defaultFormatter": "ms-python.python"
	},
	"[rust]": {
		"editor.defaultFormatter": "statiolake.vscode-rustfmt"
	},
	"gitlens.advanced.messages": {
		"suppressImproperWorkspaceCasingWarning": true
	},
	"workbench.colorTheme": "GitHub Dark"
}
